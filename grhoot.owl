            </owl:Restriction>
        </rdf:type>
        <ont:einWeiteresElementDerEntgegengesetztenBedeutungWirdHinzugefuegt rdf:resource="https://ramonakuehn.de/grhoot.owl#Ganze-Nachfolgend"/>
        <ont:einWeiteresElementDerEntgegengesetztenBedeutungWirdHinzugefuegt rdf:resource="https://ramonakuehn.de/grhoot.owl#Phrasenobjekt"/>
        <ont:istLinguistischeGruppe rdf:resource="https://ramonakuehn.de/grhoot.owl#Semantisch"/>
