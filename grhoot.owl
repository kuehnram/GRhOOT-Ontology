        <ont:Name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Etymologische Figur</ont:Name>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Verbindung zweier Woerter desselben Wortstammes, aber verschiedener Wortklassen, z.B. Verb und Nomen (Berner)</rdfs:comment>
        <ont:istBeispiel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&quot;Gar schoene Spiele spiel ich mit dir&quot; (Goethe: Erlkoenig)</ont:istBeispiel>
        <ont:istBeispiel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&quot;Wer andern eine Grube graebt, faellt selbst hinein&quot; (Sprichwort)</ont:istBeispiel>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Etymologische Figur</rdfs:label>
        <rdfs:seeAlso xml:lang="en">Etymological figure</rdfs:seeAlso>
    </owl:NamedIndividual>
    


