        <ont:Name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sarkasmus</ont:Name>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">beißender, verletzender Spott, Hohn, der jemanden, etwas laecherlich machen will (Duden)</rdfs:comment>
        <ont:istBeispiel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&quot;Fuer alle Autofahrer, die nur Singen und Klatschen in der Schule hatten: Dies ist eine Ein- und Ausfahrt. Hier fahren Autos rein und raus!&quot; (Schild in einer Einfahrt)</ont:istBeispiel>
        <ont:istBeispiel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">“Gab es das Kleid nicht in deiner Groeße?”</ont:istBeispiel>
        <ont:istBeispiel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">“Hast du deine Stelzen vergessen?”</ont:istBeispiel>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sarkasmus</rdfs:label>
        <rdfs:seeAlso xml:lang="en">Sarcasm</rdfs:seeAlso>
