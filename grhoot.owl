        <rdfs:seeAlso xml:lang="sr">Poredjenje</rdfs:seeAlso>
        <rdfs:seeAlso xml:lang="en">Simile</rdfs:seeAlso>
        <rdfs:seeAlso xml:lang="la">Similitudo</rdfs:seeAlso>
    </owl:NamedIndividual>
    


